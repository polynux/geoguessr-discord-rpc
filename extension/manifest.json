{
  "manifest_version": 2,
  "name": "Geoguessr Discord-RPC",
  "version": "0.1",
  "permissions": ["tabs", "https://www.geoguessr.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.geoguessr.com/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "geoguessr.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
